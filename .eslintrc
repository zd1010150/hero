{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "classes": true,
    }
  },
  "extends": "eslint-config-airbnb",
  "rules": {
    "indent": ["error",2],
    "no-trailing-spaces": ["error", { "skipBlankLines": true }],
    "func-names": ["error", "never"],
    },

}
